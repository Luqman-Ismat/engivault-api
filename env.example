# Environment Configuration
NODE_ENV=development

# Server Configuration
PORT=3000
HOST=0.0.0.0

# Logging
LOG_LEVEL=info
LOG_PRETTY_PRINT=true
REDACT_PII=true

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_TIME_WINDOW=60000

# Caching
CACHE_TTL=600000

# Performance Settings
ENABLE_COMPRESSION=true
ENABLE_CACHING=true

# Monitoring
ENABLE_METRICS=true
