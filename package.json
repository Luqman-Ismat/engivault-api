{
  "name": "engivault-api",
  "version": "1.0.0",
  "description": "Engineering calculations API for fluid dynamics and pump systems",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec tsx src/index.ts",
    "dev:clean": "pkill -f 'tsx src/index.ts' || true && sleep 1 && npm run dev",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "generate-sdk": "node scripts/generate-sdk.js",
    "sdk:install-ts": "cd clients/ts && npm install",
    "sdk:install-py": "cd clients/py && pip3 install -e .",
    "sdk:test-ts": "cd clients/ts && npm run build",
    "sdk:test-py": "cd clients/py && python3 example.py"
  },
  "keywords": [
    "engineering",
    "calculations",
    "fluid-dynamics",
    "pump-systems",
    "api"
  ],
  "author": "Luqman Ismat",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.23.1",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.0",
    "openapi-typescript-codegen": "^0.29.0",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@fastify/caching": "^8.0.0",
    "@fastify/compress": "^8.1.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "fastify": "^5.5.0",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "prom-client": "^15.1.3",
    "zod": "^4.0.17",
    "zod-to-json-schema": "^3.24.6"
  }
}
