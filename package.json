{
  "name": "engivault-api",
  "version": "2.0.0",
  "description": "Simplified Engineering Calculations API for Railway deployment",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "lint": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:deploy": "npx prisma migrate deploy",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:studio": "npx prisma studio",
    "setup": "npm run db:generate && npm run db:migrate && npm run db:seed"
  },
  "keywords": [
    "engineering",
    "calculations",
    "api",
    "railway"
  ],
  "author": "Luqman Ismat",
  "license": "ISC",
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.2.0",
    "@prisma/client": "^6.16.1",
    "bcryptjs": "^3.0.2",
    "fastify": "^4.29.1",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "prisma": "^6.16.1",
    "zod": "^3.24.1"
  }
}
