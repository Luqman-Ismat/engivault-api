{
  "name": "engivault",
  "version": "1.0.0",
  "description": "Official JavaScript/TypeScript SDK for EngiVault - Engineering calculations made simple",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./fluid-mechanics": {
      "import": "./dist/modules/fluid-mechanics.esm.js",
      "require": "./dist/modules/fluid-mechanics.js",
      "types": "./dist/modules/fluid-mechanics.d.ts"
    },
    "./heat-transfer": {
      "import": "./dist/modules/heat-transfer.esm.js",
      "require": "./dist/modules/heat-transfer.js",
      "types": "./dist/modules/heat-transfer.d.ts"
    },
    "./hydraulics": {
      "import": "./dist/modules/hydraulics.esm.js",
      "require": "./dist/modules/hydraulics.js",
      "types": "./dist/modules/hydraulics.d.ts"
    },
    "./pumps": {
      "import": "./dist/modules/pumps.esm.js",
      "require": "./dist/modules/pumps.js",
      "types": "./dist/modules/pumps.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "docs": "typedoc src/index.ts --out docs",
    "size": "bundlesize"
  },
  "keywords": [
    "engineering",
    "calculations",
    "fluid-mechanics",
    "heat-transfer",
    "hydraulics",
    "pumps",
    "api",
    "sdk",
    "typescript",
    "javascript"
  ],
  "author": "EngiVault Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Luqman-Ismat/engivault-api.git",
    "directory": "packages/engivault-js"
  },
  "homepage": "https://engivault.com",
  "bugs": {
    "url": "https://github.com/Luqman-Ismat/engivault-api/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "bundlesize": "^0.18.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.5.0",
    "rollup-plugin-dts": "^6.1.0",
    "@rollup/plugin-terser": "^0.4.4",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.4",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "zod": "^3.22.4"
  },
  "bundlesize": [
    {
      "path": "dist/index.js",
      "maxSize": "50 kB"
    },
    {
      "path": "dist/index.esm.js",
      "maxSize": "50 kB"
    }
  ]
}
